                                                                                <div class="price_slider_amount">
                                                                                    <input type="text" id="width-slider" name="width" placeholder="Add Your Width" />
                                                                                </div><!--/.price_slider_amount-->
                                                                            </div><!--/.price-filter-->
                                                                        </div><!--/.info_widget-->
                                                                    </div><!--/.travel-filter-->
                                                                </div><!--/.col-->
                                                            </div><!--/.row-->
                                                        </div><!--/.travel-budget-->
                                                    </div><!--/.col-->
                                                
                                                
                                                    <div class="col-sm-7">
                                                        <div class="about-btn travel-mrt-0 pull-right">
                                                            <button  class="about-view travel-btn">
                                                                search	
                                                            </button><!--/.travel-btn-->
                                                        </div><!--/.about-btn-->
                                                    </div><!--/.col-->

                                                </div><!--/.row-->
        
                                            </div><!--/.tab-para-->
                                        </form>
                                    </div><!--/.tabpannel-->
        
                                </div><!--/.tab content-->
                            </div><!--/.desc-tabs-->
                        </div><!--/.single-travel-box-->
                    </div><!--/.col-->
                </div><!--/.row-->
            </div><!--/.container-->
        
        </section><!--/.travel-box-->

