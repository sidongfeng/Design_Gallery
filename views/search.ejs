                                                                                <div id="slider-range"></div><!--/.slider-range-->

                                                                                <div class="price_slider_amount">
                                                                                    <input type="text" id="height-slider" name="height"  placeholder="Add Your Height" />
                                                                                </div><!--/.price_slider_amount-->
                                                                            </div><!--/.price-filter-->
                                                                        </div><!--/.info_widget-->
                                                                    </div><!--/.travel-filter-->
                                                                </div><!--/.col-->
                                                            </div><!--/.row-->
                                                        </div><!--/.travel-budget-->

